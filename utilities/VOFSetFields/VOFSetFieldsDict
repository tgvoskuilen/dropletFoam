FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      VOFSetFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultSpecies
(
    N2   1.0
);

droplets
(
    DropMMH
    {
        center   (0 0.06 0);
        radius   (0.01 0.01 1e15);
        delVapor 2e-5;
        U        (0 0 -1);
        liquidSpecies
        (
            CH3NHNH2L  1.0
        );
        vaporSpecies
        (
            CH3NHNH2   0.025
        );
    }
    
    DropNTO
    {
        center   (0 0.06 0);
        radius   (0.01 0.01 1e15);
        delVapor 2e-5;
        U        (0 0 -1);
        liquidSpecies
        (
            N2O4L  1.0
        );
        vaporSpecies
        (
            N2O4   0.58;
            NO2    0.09;
        );
    }

);


