FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 1e5;

boundaryField
{
    atmosphere
    {
        type            waveTransmissive;
        value           uniform 1e5;
        phi             phi;
        rho             rho;
        gamma           1.4;
        lInf            0.05;
        fieldInf        1e5;
    }


    sides
    {
        type            symmetryPlane;
    }
    
    heatedWall
    {
        type            zeroGradient;
    }
    
    defaultFaces
    {
        type            empty;
    }
}

// ************************************************************************* //
